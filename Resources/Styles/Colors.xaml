<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="secondary_color" Color="#171717"></SolidColorBrush>
    <LinearGradientBrush x:Key="main_color" StartPoint="0.8,1" EndPoint="1, 0">
        <GradientStop Color="#3ab4ed" Offset="-0.2"></GradientStop>
        <GradientStop Color="#145ba0" Offset="0.4"></GradientStop>
    </LinearGradientBrush>
    <SolidColorBrush x:Key="text_color" Color="White"></SolidColorBrush>
    <SolidColorBrush x:Key="help_color" Color="#2b2b2b"></SolidColorBrush>
    <SolidColorBrush x:Key="fourth_color" Color="#FF3F3F3F"/>
    <SolidColorBrush x:Key="Border" Color="#888888"/>
</ResourceDictionary>